{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "scripts/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "array-type": [true, "generic"],
    "ordered-imports": [false],
    "triple-equals": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "no-debugger": false,
    "noImplicitAny": false
  }
}
